// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Picture{
  id Int @id @default(autoincrement())
  link String @unique 
}
model Event{
  id Int @id @default(autoincrement())
  name String
  due_date Int 
  description String
  image String
  special String @unique
}