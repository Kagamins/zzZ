generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("URL_DATABASE")
}

model Picture {
  id   Int    @id @default(autoincrement())
  link String @unique
}

model Event {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  image       String
  special     String @unique
}
